# SOL VM Communication Configuration
# Copy this file to .env.local and update the values

# Executor Configuration
# Options: 'auto', 'local', 'hybrid'
# 'auto' will try SOL VM first, then fallback to local
REACT_APP_EXECUTOR_TYPE=auto

# SOL VM Message Queue API Configuration
# Replace these with your actual deployed API URLs and credentials
REACT_APP_TASK_QUEUE_URL=https://your-message-queue-api.herokuapp.com/tasks
REACT_APP_RESULT_QUEUE_URL=https://your-message-queue-api.herokuapp.com/results
REACT_APP_API_KEY=your-secret-api-key

# Polling and Retry Configuration
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=2000
REACT_APP_POLL_INTERVAL=3000

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_AUTO_REFRESH_STATUS=true
